{
  "name": "festarter",
  "description": "festarter",
  "version": "0.0.1",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build": "yarn prod",
    "start": "yarn dev-server",
    "start:nodemon": "nodemon --watch ./package.json --watch ./.env --watch ./webpack.config.js --watch ./src/views/site --exec yarn start",
    "dev": "webpack --mode=development --stats-children",
    "dev-server": "webpack serve --mode=development",
    "dev-test": "webpack --mode=development --stats-children --env is-test",
    "dev-test-server": "webpack serve --mode=development --env is-test",
    "prod": "webpack --mode=production",
    "prod-server": "webpack serve --mode=production",
    "prod-test": "webpack --mode=production --env is-test",
    "prod-test-server": "webpack serve --mode=production --env is-test",
    "clean": "rm -rf dist",
    "analyze": "webpack --mode=production --env analyze",
    "analyze:server": "webpack serve --mode=production --env analyze",
    "eslintJs": "eslint src/**/*.js config/**/*.js",
    "eslintTs": "eslint src/**/*.ts config/**/*.ts",
    "tsc:build": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "expose-loader": "^4.1.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^7.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "pug": "^3.0.2",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "simple-nunjucks-loader": "3.2.0",
    "simple-pug-loader": "^0.2.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@icon/icofont": "^1.0.1-alpha.1",
    "axios": "^1.3.1",
    "dotenv": "^16.3.1",
    "nunjucks": "^3.2.4",
    "swiper": "^8.4.6",
    "web-vitals": "^3.1.1",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/muhammedozalp/festarter-v1.git"
  },
  "keywords": [],
  "author": {
    "name": "Muhammed Ozalp",
    "email": "muhammedozalp@gmail.com"
  },
